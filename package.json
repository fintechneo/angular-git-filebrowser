{
  "name": "angular-filebrowser",
  "version": "0.0.55",
  "license": "MIT",
  "description": "Angular filebrowser backed by git repository in the browser",
  "homepage": "https://github.com/fintechneo/angular-git-filebrowser",
  "bugs": {
    "url": "https://github.com/fintechneo/angular-git-filebrowser/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-local-git-server": "tsc -p e2e/tsconfig.e2e.json && node out-tsc/e2e/localgitserver/localgitserver.main.js",
    "start-local-git": "ng serve --proxy-config proxy.conf.localgit.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib": "node ngmakelib-conf.js",
    "libwatch": "node ngmakelib-conf.js --watch",
    "publishlib": "cd .ngmakelibtmp/dist && npm publish",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "deep-diff": "^1.0.2",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.2.2",
    "tsickle": "^0.34.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.0",
    "cgi": "^0.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ngmakelib": "^0.2.5",
    "ngx-i18nsupport": "^0.16.2",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "3.2.4"
  }
}
